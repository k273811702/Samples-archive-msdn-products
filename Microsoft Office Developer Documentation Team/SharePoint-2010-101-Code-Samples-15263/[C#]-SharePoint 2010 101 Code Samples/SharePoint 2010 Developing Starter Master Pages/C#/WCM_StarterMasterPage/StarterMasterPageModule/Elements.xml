<?xml version="1.0" encoding="utf-8"?>
<!-- This module deploys the starter master page to the masterpage gallery.
	 A starter master page is one with no styling or images but all the content
	 place holders required for full SharePoint functionality. You can use this
	 page as a starter to build your own, branded master pages. -->
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <!-- Use the Url attribute to place files in the masterpage gallery -->
  <Module Name="StarterMasterPageModule" Url="_catalogs/masterpage">
  <File Path="StarterMasterPageModule\_starter_publishing.master" Url="_starter_publishing.master" Type="GhostableInLibrary" />
</Module>
</Elements>