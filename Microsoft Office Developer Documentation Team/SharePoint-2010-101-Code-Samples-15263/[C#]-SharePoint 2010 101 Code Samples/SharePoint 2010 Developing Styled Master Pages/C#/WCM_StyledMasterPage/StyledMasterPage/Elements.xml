<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
	<!-- This module deploys a complete master page to the masterpage gallery. -->
	<Module Name="StyledMasterPage" Url="_catalogs/masterpage">
		<File Path="StyledMasterPage\StyledExample.master" Url="StyledExample.master" />
	</Module>
	<!-- This module deploys the examplestyles.css sheet to the Style Library -->
	<Module Name="ExampleStyles" Url="Style Library/ExampleStyles">
		<File Path="StyledMasterPage\examplestyles.css" Url="examplestyles.css" />		
	</Module>
</Elements>