@model ManagingTasksUsingEWS.Models.UserTasksModel

    <table style="width:100%;border:solid">

        <tr>
            <td style="width:20%;font-weight:bold">Task Subject</td>
            <td style="width:70%;font-weight:bold">Task Message</td>
            <td style="width:10%;font-weight:bold">Start Date</td>
        </tr>
   
    @foreach (var task in Model.Tasks)
    {
       <tr>
           <td>@task.TaskTitle</td>
           <td>@task.TaskMessage</td>
           <td>@task.TaskStartDate</td>
       </tr>
    }
 </table>

