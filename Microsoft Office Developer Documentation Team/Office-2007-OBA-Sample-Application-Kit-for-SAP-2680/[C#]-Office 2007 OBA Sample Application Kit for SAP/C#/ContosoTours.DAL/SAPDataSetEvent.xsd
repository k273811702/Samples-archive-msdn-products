<?xml version="1.0" standalone="yes"?>
<xs:schema id="SAPDataSetEvent" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="SAPDataSetEvent" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Event">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="EventID" type="xs:int" />
              <xs:element name="VenueID" type="xs:int" />
              <xs:element name="EventName">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="255" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="EventDescription">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="4000" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="EventPhoto" type="xs:base64Binary" />
              <xs:element name="EventDate" type="xs:dateTime" />
              <xs:element name="GoldPackagePrice" type="xs:decimal" />
              <xs:element name="SilverPackagePrice" type="xs:decimal" />
              <xs:element name="BronzePackagePrice" type="xs:decimal" />
              <xs:element name="GoldPackageTrueCost" type="xs:decimal" />
              <xs:element name="SilverPackageTrueCost" type="xs:decimal" />
              <xs:element name="BronzePackageTrueCost" type="xs:decimal" />
              <xs:element name="EventTotalCost" type="xs:decimal" />
              <xs:element name="EventTypeName">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="255" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="VenueName">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="255" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>